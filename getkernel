curl -fLO https://cdn.kernel.org/pub/linux/kernel/v5.x/linux-5.14.11.tar.xz
cd linux*
curl https://raw.githubusercontent.com/Mederim/dotfiles/main/kernel/.config > .config 
# path da applicare
sed '/<stdlib.h>/a #include <linux/stddef.h>' tools/objtool/arch/x86/decode.c > _                               |
mv -f _ tools/objtool/arch/x86/decode.c